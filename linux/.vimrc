set nocompatible
" 鼠标支持
set mouse=a
" 编辑器
syntax on
filetype indent on
set t_Co=256
set encoding=utf-8
set number
set autoindent
set tabstop=4
set shiftwidth=4
set expandtab
set softtabstop=4
set wrap
" 补充组件
set ruler
set laststatus=1
set showmode
set showcmd
" 搜索
set showmatch
set hlsearch
" 编辑器行为
set nobackup
set noswapfile
set autoread
